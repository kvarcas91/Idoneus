<Window x:Class="IdoneusV2.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IdoneusV2"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Top" Content="Add" Click="Button_Click"/>
        <ContentControl DockPanel.Dock="Left" prism:RegionManager.RegionName="SideBarRegion" />
        <Grid>
            <TextBlock Text="CENTRAS" HorizontalAlignment="Center"
                       VerticalAlignment="Center"/>
        </Grid>
    </DockPanel>
</Window>
